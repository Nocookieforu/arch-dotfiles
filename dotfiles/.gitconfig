
# Added some new information based on the book Git Workflows by Yan Pritzker
# http://documentup.com/skwp/git-workflows-book

# tree alias grabbed from:
# stackoverflow.com/questions/1064361/unable-to-show-a-git-tree-in-terminal 
[alias]
    tree = log --graph --decorate --pretty=oneline --abbrev-commit
    ls = log --max-count=10 --pretty=oneline --stat --date=relative
    br = branch -va
    rem = remote -v
    unstage = reset HEAD
    uncommit = reset --soft HEAD^

[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = auto

[gc]
    auto = 1

[merge]
    summary = true

